      "bbb21ffe-6315-4651-926d-ebcee602b790": {
        "id": "bbb21ffe-6315-4651-926d-ebcee602b790",
        "message": {
          "id": "bbb21ffe-6315-4651-926d-ebcee602b790",
          "author": {
            "role": "user",
