      "bbb2155c-413a-481b-b644-182a2499ada3": {
        "id": "bbb2155c-413a-481b-b644-182a2499ada3",
        "message": {
          "id": "bbb2155c-413a-481b-b644-182a2499ada3",
          "author": {
            "role": "user",
